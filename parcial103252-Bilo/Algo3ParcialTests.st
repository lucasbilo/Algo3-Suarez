TestCase subclass: #AuxilioTest	instanceVariableNames: ''	classVariableNames: ''	package: 'Algo3ParcialTests'!!AuxilioTest methodsFor: 'tests' stamp: 'Lucas 12/1/2020 20:27'!test04AuxilioConCasoASPONoPuedeCargarCantidadNegativa	| unAuxilio|	unAuxilio := Auxilio new.	unAuxilio setFaseCovid: ASPO new.		self should: [unAuxilio cargarCantidadPasajeros: -1] raise: ErrorCantidadDePasajerosNegativa.	self should: [unAuxilio cargarCantidadPasajeros: -2] raise: ErrorCantidadDePasajerosNegativa.	self should: [unAuxilio cargarCantidadPasajeros: -3] raise: ErrorCantidadDePasajerosNegativa.! !!AuxilioTest methodsFor: 'tests' stamp: 'Lucas 12/1/2020 20:55'!test06AuxilioConCasoNORMALDebePoderCargarHastaDOSPasajeros	| unAuxilio|	unAuxilio := Auxilio new.	unAuxilio setFaseCovid: NORMAL new.		unAuxilio cargarCantidadPasajeros: 1.	self assert: ( unAuxilio cantidadPasajeros) equals: 1.		unAuxilio cargarCantidadPasajeros: 2.	self  assert: (unAuxilio cantidadPasajeros) equals: 2.! !!AuxilioTest methodsFor: 'tests' stamp: 'Lucas 12/1/2020 20:53'!test03AuxilioConCasoDISPONoDebePoderCargarMasDePasajero	| unAuxilio|	unAuxilio := Auxilio new.		unAuxilio setFaseCovid: DISPO new.	self should: [ unAuxilio cargarCantidadPasajeros: 2 ] raise: FaseDISPONoPuedeCargarMasDeUnPasajero.! !!AuxilioTest methodsFor: 'tests' stamp: 'Lucas 12/1/2020 20:22'!test01AuxilioConCasoASPONoDebePoderCargarPasajeros	| unAuxilio|	unAuxilio := Auxilio new.		unAuxilio setFaseCovid: ASPO new.	self should: [unAuxilio cargarCantidadPasajeros: 1] raise: FaseASPONoPuedeCargarNingunPasajero.	self should: [unAuxilio cargarCantidadPasajeros: 2] raise: FaseASPONoPuedeCargarNingunPasajero.	self should: [unAuxilio cargarCantidadPasajeros: 3] raise: FaseASPONoPuedeCargarNingunPasajero.! !!AuxilioTest methodsFor: 'tests' stamp: 'Lucas 12/1/2020 20:20'!testAuxilio	| unAuxilio|	unAuxilio := Auxilio new.		unAuxilio setFaseCovid: ASPO new.	self should: [unAuxilio cargarCantidadPasajeros: 1] raise: FaseASPONoPuedeCargarNingunPasajero.		unAuxilio setFaseCovid: DISPO new.	unAuxilio cargarCantidadPasajeros: 1.	self should: [unAuxilio cantidadPasajeros = 1].	self should: [unAuxilio cargarCantidadPasajeros: 2] raise: FaseDISPONoPuedeCargarMasDeUnPasajero.		unAuxilio setFaseCovid: NORMAL new.	unAuxilio cargarCantidadPasajeros: 2.	self should: [ unAuxilio cantidadPasajeros = 2].	self should: [unAuxilio cargarCantidadPasajeros: 3] raise: FaseNORMALNoPuedeCargarMasDeDosPasajeros.			! !!AuxilioTest methodsFor: 'tests' stamp: 'Lucas 12/1/2020 20:56'!test08AuxilioConCasoNORMALONoPuedeCargarCantidadNegativa	| unAuxilio|	unAuxilio := Auxilio new.	unAuxilio setFaseCovid: NORMAL new.		self should: [unAuxilio cargarCantidadPasajeros: -1] raise: ErrorCantidadDePasajerosNegativa.	self should: [unAuxilio cargarCantidadPasajeros: -2] raise: ErrorCantidadDePasajerosNegativa.	self should: [unAuxilio cargarCantidadPasajeros: -3] raise: ErrorCantidadDePasajerosNegativa.! !!AuxilioTest methodsFor: 'tests' stamp: 'Lucas 12/1/2020 20:52'!test02AuxilioConCasoDISPODebePoderCargarUnPasajero	| unAuxilio|	unAuxilio := Auxilio new.		unAuxilio setFaseCovid: DISPO new.	unAuxilio cargarCantidadPasajeros: 1.	self assert: (unAuxilio cantidadPasajeros) equals: 1.! !!AuxilioTest methodsFor: 'tests' stamp: 'Lucas 12/1/2020 20:55'!test07AuxilioConCasoNORMALNODebePoderCargarMasDOSPasajeros	| unAuxilio|	unAuxilio := Auxilio new.	unAuxilio setFaseCovid: NORMAL new.		self should: [ unAuxilio cargarCantidadPasajeros: 3 ] raise: FaseNORMALNoPuedeCargarMasDeDosPasajeros.! !!AuxilioTest methodsFor: 'tests' stamp: 'Lucas 12/1/2020 20:27'!test05AuxilioConCasoDISPONoPuedeCargarCantidadNegativa	| unAuxilio|	unAuxilio := Auxilio new.	unAuxilio setFaseCovid: DISPO new.		self should: [unAuxilio cargarCantidadPasajeros: -1] raise: ErrorCantidadDePasajerosNegativa.	self should: [unAuxilio cargarCantidadPasajeros: -2] raise: ErrorCantidadDePasajerosNegativa.	self should: [unAuxilio cargarCantidadPasajeros: -3] raise: ErrorCantidadDePasajerosNegativa.! !