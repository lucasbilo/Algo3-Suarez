TestCase subclass: #TestBomberman	instanceVariableNames: ''	classVariableNames: ''	package: 'BomberMan-Test'!!TestBomberman methodsFor: 'tests' stamp: 'DS 11/6/2020 07:59'!test02BombermanDaUnPasoHaciaLugarOcupadoPorParedMantieneSuPosicion	| bomberman unLugar lugarOcupado |		unLugar := Lugar2 new.	bomberman := BomberMan nace: (unLugar). 		lugarOcupado := Lugar2 con: (Pared new).		self should: [  bomberman moverA: lugarOcupado ] raise: PosicionOcupada .		self deny: [ bomberman estaEn: lugarOcupado ] "bomberman estaEn: otroLugar".		! !!TestBomberman methodsFor: 'tests' stamp: 'DS 11/5/2020 20:52'!test01BombermanDaUnPasoHaciaLugarVacioSigueVivo	| bomberman otroLugar |		otroLugar := Lugar2 new.		bomberman := BomberMan nace: (Lugar2 new). 	bomberman moverA: otroLugar.		self assert: [bomberman estaEn: otroLugar].	! !