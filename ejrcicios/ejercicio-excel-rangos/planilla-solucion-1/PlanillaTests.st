TestCase subclass: #PlanillaTest	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PlanillaTests'!!PlanillaTest methodsFor: 'tests' stamp: 'a 9/12/2019 17:41'!testPlanillaPermiteColocarDatosEnCelda	|unaPlanilla|	unaPlanilla := Planilla new.	unaPlanilla colocarEnCelda: 'C3' elValor: 5.	self assert: (unaPlanilla getValorEnCelda: 'C3') equals: 5.! !!PlanillaTest methodsFor: 'tests' stamp: 'a 9/12/2019 17:42'!testPlanillaPermiteColocarDatosNumericosEnCelda	|unaPlanilla|	unaPlanilla := Planilla new.	unaPlanilla colocarEnCelda: 'C3' elValor: 5.	self assert: (unaPlanilla getValorEnCelda: 'C3') equals: 5.! !!PlanillaTest methodsFor: 'tests' stamp: 'a 9/12/2019 17:39'!testPlanillaVaciaNoTieneDatosEnLasCeldas	|unaPlanilla|	unaPlanilla := Planilla new.	self assert: (unaPlanilla getValorEnCelda: 'A2') equals: ''.! !!PlanillaTest methodsFor: 'tests' stamp: 'a 9/12/2019 17:42'!testPlanillaPermiteColocarDatosCadenaEnCelda	|unaPlanilla|	unaPlanilla := Planilla new.	unaPlanilla colocarEnCelda: 'C3' elValor: '5'.	self assert: (unaPlanilla getValorEnCelda: 'C3') equals: '5'.! !TestCase subclass: #RangosTests	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PlanillaTests'!!RangosTests methodsFor: 'tests' stamp: 'a 9/12/2019 18:28'!testPuedoCrearRangoHorizontalVacio	|unaPlanilla unRango|	unaPlanilla := Planilla new.	unRango := unaPlanilla crearRangoHorizontalDesde: 'A2' hasta: 'C2'.	self assert: (unRango getCantidadCeldasUsadas = 0).! !