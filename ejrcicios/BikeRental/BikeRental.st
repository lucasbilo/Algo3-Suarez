TestCase subclass: #PruebasBikeRental	instanceVariableNames: ''	classVariableNames: ''	package: 'BikeRental-Tests'!!PruebasBikeRental methodsFor: 'tests' stamp: 'tomas 6/25/2020 18:47'!test02AlquilerBiciPorPeriodoDe5| unaBici unAlquiler unCliente |unaBici := Bici new.unaBici establecerPrecio: 3.unAlquiler := Alquiler crearAlquiler: unaBici .unAlquiler establecerComoDiario: false.unAlquiler establecerPeriodo: 5.unCliente := Cliente new.unCliente agregarAlquiler: unAlquiler.self assert: unCliente saldoAPagar equals: 15.! !!PruebasBikeRental methodsFor: 'tests' stamp: 'tomas 6/25/2020 19:02'!test01PruebasBicicleta1| unaBici unAlquiler unCliente |unaBici := Bici new.unaBici establecerPrecioPorDia: 10.unAlquiler := Alquiler crearAlquiler: unaBici .unAlquiler establecerComoDiario: true.unCliente := Cliente new.unCliente agregarAlquiler: unAlquiler.self assert: unCliente saldoAPagar equals: 10.! !Object subclass: #Alquiler	instanceVariableNames: 'bici periodo esDiario'	classVariableNames: ''	package: 'BikeRental'!!Alquiler methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:01'!esDiario	^ esDiario .! !!Alquiler methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:03'!establecerComoDiario: diario	esDiario := diario  .! !!Alquiler methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:01'!obtenerBici	^ bici .! !!Alquiler methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:01'!establecerPeriodo: unPeriodo	periodo := unPeriodo  .! !!Alquiler methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:20'!establecerBici: unaBici	bici := unaBici .! !!Alquiler methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:02'!obtenerPeriodo	^ periodo .! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Alquiler class	instanceVariableNames: ''!!Alquiler class methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:53'!crearAlquiler: unaBici	| nuevoAlquiler |		nuevoAlquiler := Alquiler new.	nuevoAlquiler establecerBici: unaBici.		^ nuevoAlquiler! !Object subclass: #Bici	instanceVariableNames: 'precio precioPorDia'	classVariableNames: ''	package: 'BikeRental'!!Bici methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 09:57'!obtenerPrecioPorDia	^ precioPorDia .! !!Bici methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 09:56'!establecerPrecio: unPrecio	precio := unPrecio.! !!Bici methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 09:57'!obtenerPrecio	^ precio.! !!Bici methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 09:56'!establecerPrecioPorDia: unPrecio	precioPorDia := unPrecio.! !Object subclass: #Cliente	instanceVariableNames: 'alquileres'	classVariableNames: ''	package: 'BikeRental'!!Cliente methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:23'!agregarAlquiler: unAlquiler	alquileres add: unAlquiler .! !!Cliente methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:58'!initialize	super initialize.		alquileres := OrderedCollection new.! !!Cliente methodsFor: 'as yet unclassified' stamp: 'PabloSuarez 6/29/2016 10:30'!saldoAPagar	| resultado |		resultado := 0.		alquileres do: [ :a | 							( a esDiario ) ifTrue: 									[ resultado := resultado + ((a obtenerBici ) obtenerPrecioPorDia) ]								ifFalse:									[ resultado := resultado + (((a obtenerBici) obtenerPrecio ) * a obtenerPeriodo ) ]			 ].			^ resultado! !